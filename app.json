{
  "expo": {
    "name": "Group Sing Along",
    "slug": "group-sing-along-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "groupsingalong",
    "newArchEnabled": true,
    "splash": {
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "org.cybergoose.groupsingalong",
      "deploymentTarget": "16.0",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "org.cybergoose.groupsingalong"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      [
        "react-native-iap",
        {
          "ios": {
            "enableStoreKit2": false
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "d03eb0b3-2c58-4ad9-a653-2ecba7ba1f9c"
      }
    }
  }
}
